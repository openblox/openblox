			<foaf:homepage rdf:resource="https://johnmh.me/" />
		</foaf:Person>
		<foaf:Person>
			<foaf:name>Mark Otaris</foaf:name>
			<foaf:mbox rdf:resource="mailto:mark@openblox.org" />
			<foaf:homepage rdf:resource="https://zero-one.tk/~mark/" />
		</foaf:Person>
